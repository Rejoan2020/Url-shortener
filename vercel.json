{
"version": 2,
"builds":[
    {
        "src": "Url-shortener/wsgi.py",
        "use": "@vercel/python",
        "config":{ "maxLamdaSize" : "15mb", "runtime":"python3.9"}
    },
    {
        "src" : "build_files.sh",
        "use" : "@vercel/static-build",
        "config" : {
            "distDir" : "staticfiles_build"
        }
    }
],
"routes":[
    {
        "src":"/",
        "dest":"/"
    },
    {
        "src":"/test",
        "dest":"/"
    }
]
}
